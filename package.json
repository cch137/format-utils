{
  "name": "@cch137/format-utils",
  "version": "0.1.11",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "devbuild": "tsc -w",
    "devstart": "nodemon ./dist/index.test.js",
    "dev": "npm-run-all --parallel devbuild devstart",
    "build": "npm i && tsc",
    "start": "node ./dist/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cch137/format-utils"
  },
  "keywords": [
    "cch137",
    "format",
    "utils"
  ],
  "author": "cch137",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.0"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./bigmap": {
      "types": "./dist/bigmap/index.d.ts",
      "default": "./dist/bigmap/index.js"
    },
    "./bytes": {
      "types": "./dist/bytes/index.d.ts",
      "default": "./dist/bytes/index.js"
    },
    "./date": {
      "types": "./dist/date/index.d.ts",
      "default": "./dist/date/index.js"
    },
    "./deep-equal": {
      "types": "./dist/deep-equal/index.d.ts",
      "default": "./dist/deep-equal/index.js"
    },
    "./detect-languages": {
      "types": "./dist/detect-languages/index.d.ts",
      "default": "./dist/detect-languages/index.js"
    },
    "./fetch2": {
      "types": "./dist/fetch2/index.d.ts",
      "default": "./dist/fetch2/index.js"
    },
    "./numeric-compare": {
      "types": "./dist/numeric-compare/index.d.ts",
      "default": "./dist/numeric-compare/index.js"
    },
    "./objects": {
      "types": "./dist/objects/index.d.ts",
      "default": "./dist/objects/index.js"
    },
    "./read-stream": {
      "types": "./dist/read-stream/index.d.ts",
      "default": "./dist/read-stream/index.js"
    },
    "./safe-filename": {
      "types": "./dist/safe-filename/index.d.ts",
      "default": "./dist/safe-filename/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./sleep": {
      "types": "./dist/sleep/index.d.ts",
      "default": "./dist/sleep/index.js"
    },
    "./urls": {
      "types": "./dist/urls/index.d.ts",
      "default": "./dist/urls/index.js"
    },
    "./youtube": {
      "types": "./dist/youtube/index.d.ts",
      "default": "./dist/youtube/index.js"
    },
    "./throttle-debounce": {
      "types": "./dist/throttle-debounce/index.d.ts",
      "default": "./dist/throttle-debounce/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "bigmap": [
        "dist/bigmap/index.d.ts"
      ],
      "bytes": [
        "dist/bytes/index.d.ts"
      ],
      "date": [
        "dist/date/index.d.ts"
      ],
      "deep-equal": [
        "dist/deep-equal/index.d.ts"
      ],
      "detect-languages": [
        "dist/detect-languages/index.d.ts"
      ],
      "fetch2": [
        "dist/fetch2/index.d.ts"
      ],
      "numeric-compare": [
        "dist/numeric-compare/index.d.ts"
      ],
      "objects": [
        "dist/objects/index.d.ts"
      ],
      "read-stream": [
        "dist/read-stream/index.d.ts"
      ],
      "safe-filename": [
        "dist/safe-filename/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ],
      "sleep": [
        "dist/sleep/index.d.ts"
      ],
      "urls": [
        "dist/urls/index.d.ts"
      ],
      "youtube": [
        "dist/youtube/index.d.ts"
      ],
      "throttle-debounce": [
        "dist/throttle-debounce/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  }
}
